@model List<AnalysisVM>

@{
    int x = 1;
}
@foreach (var item in Model)
{



    @if (item.IsAdd ==true)
    {
        <div class="row">
            <div class="col-sm-2"></div>
            <div class="col-sm-6">

                @x - Analysis
                @{
                    x++;
                }
            </div>
            <div class="col-sm-4">  <a style="color:var(--six); font-weight: bold;background-image: linear-gradient(90deg, #f5f5f527 0%, #2bc4b7 75%);width:100%;width:60px ;padding: 10px;border: none; border-radius: 4px;" href="~/Files/Docs/@item.AnalysisPDF" download="download"><i class="fa-solid fa-download fa-bounce"></i></a></div>
        </div>
        <br />
        <hr />
    }
   
   
}


